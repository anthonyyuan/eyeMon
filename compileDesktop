function compile() {
    g++ -std=c++11 -Werror $1 -o $2 -I. $(pkg-config --cflags --libs opencv)
}

cd jni
compile "eyelike/findEyeCorner.cpp eyelike/findEyeCenter.cpp eyelike/helpers.cpp common.cpp common_settings_comp.cpp main.cpp blinkmeasure.cpp" ../el

if [ $? -eq 0 ]; then
    if [ "$1" == co ]; then
        exit 0
    fi
    ../el
fi

